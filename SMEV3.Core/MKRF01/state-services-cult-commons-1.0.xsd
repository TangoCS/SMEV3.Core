					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ApartmentBlock" type="tns:string-5" minOccurs="0" hints:rus="Дом" hints:userHint="Пример: 15">
				<xs:annotation>
					<xs:appinfo>
						<prim:exampleValues>
							<prim:example value="15"/>
						</prim:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Building" type="tns:string-5" minOccurs="0" hints:rus="Корпус" hints:userHint="Пример: 2А">
				<xs:annotation>
					<xs:appinfo>
						<prim:exampleValues>
							<prim:example value="2А"/>
						</prim:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Housing" type="tns:string-5" minOccurs="0" hints:rus="Строение" hints:userHint="Пример: 2А">
				<xs:annotation>
					<xs:appinfo>
						<prim:exampleValues>
							<prim:example value="2А"/>
						</prim:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Apartment" type="tns:string-5" minOccurs="0" hints:rus="Квартира/Офис" hints:userHint="Пример: 121">
				<xs:annotation>
					<xs:appinfo>
						<prim:exampleValues>
							<prim:example value="121"/>
						</prim:exampleValues>
