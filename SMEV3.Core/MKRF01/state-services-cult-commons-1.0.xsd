							<prim:example value="450098"/>
						</prim:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Region" type="tns:TimelineUnawareClassifierValue-Type" form="qualified" hints:classifierName="subjectRF" hints:rus="Регион" hints:userHint="Пример: Московская область"/>
			<xs:element name="District" type="tns:string-250" minOccurs="0" hints:rus="Район" hints:userHint="Пример: Октябрьский">
				<xs:annotation>
					<xs:appinfo>
						<prim:exampleValues>
							<prim:example value="Октябрьский"/>
						</prim:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="City" type="tns:string-250" hints:rus="Город/Населенный пункт" hints:userHint="Город. Пример: Москва">
				<xs:annotation>
					<xs:appinfo>
						<prim:exampleValues>
							<prim:example value="Кызыл"/>
						</prim:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="StreetText" type="tns:string-250" minOccurs="0" hints:rus="Улица" hints:userHint="Пример: улица Ленина">
				<xs:annotation>
					<xs:appinfo>
						<prim:exampleValues>
							<prim:example value="улица Ленина"/>
