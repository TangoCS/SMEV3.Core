							<xs:sequence>
								<xs:element name="Contract" type="tns:ContractType">
									<xs:annotation>
										<xs:documentation>Контракт</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Volume" type="tns:WoodVolumeType">
									<xs:annotation>
										<xs:documentation>Объем древесины</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Location" type="tns:LocationType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Местоположение</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Executor" type="tns:ContractPartyType">
