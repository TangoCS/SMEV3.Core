							<xs:sequence>
								<xs:element name="Tenant" type="tns:ContractPartyType">
									<xs:annotation>
										<xs:documentation>Арендатор</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SubTenant" type="tns:ContractPartyType" minOccurs="0" maxOccurs="unbounded">
