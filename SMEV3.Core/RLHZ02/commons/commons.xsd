<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="urn://x-artefacts-lesegais-ru/smev3/getinfo/commons/1.1.0" targetNamespace="urn://x-artefacts-lesegais-ru/smev3/getinfo/commons/1.1.0" elementFormDefault="qualified">
	<xs:annotation>
		<xs:documentation>Запросы</xs:documentation>
	</xs:annotation>
	<xs:element name="ContractStatusRequest">
		<xs:annotation>
			<xs:documentation>Запрос проверки существования документа или маркировки</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="Doc" type="tns:DocEntryType">
					<xs:annotation>
						<xs:documentation>Проверка существования документа (документ основание, отчеты, декларация о сделке)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Marking" type="tns:MarkingEntryType">
					<xs:annotation>
						<xs:documentation>Проверка существования маркировки</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocListRequest">
		<xs:annotation>
			<xs:documentation>Запрос списка документов лесопользователя</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="Company" type="tns:CompanyType">
						<xs:annotation>
							<xs:documentation>Юридическое лицо</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IP" type="tns:IPType">
						<xs:annotation>
							<xs:documentation>Индивидуальный предприниматель</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="DocType" type="tns:DocType">
					<xs:annotation>
						<xs:documentation>Тип документа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ActualFromDate" type="xs:date">
					<xs:annotation>
						<xs:documentation>Актуальность документа (начиная с даты)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocVidimusRequest">
		<xs:annotation>
			<xs:documentation>Запрос выписки по документу</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Id" type="tns:string-500" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Внутренний id документа в системе,
							при наличии поиск только по нему</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BuyerINN" type="tns:INNType">
					<xs:annotation>
						<xs:documentation>ИНН покупателя (арендатора, исполнителя)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SellerINN" type="tns:INNType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>ИНН продавца (ОИВ)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DocType" type="tns:DocType">
					<xs:annotation>
						<xs:documentation>Тип документа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element name="DeclarationNum">
						<xs:annotation>
							<xs:documentation>Номер декларации </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:length value="28"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="DocNum" type="tns:string-500">
						<xs:annotation>
							<xs:documentation>Номер документа</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="SignDate" type="xs:date">
					<xs:annotation>
						<xs:documentation>Дата подписания</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="InputDate" type="xs:date">
					<xs:annotation>
						<xs:documentation>Дата ввода в систему</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DeclaredBy" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Автор декларации (продавец/ покупатель). Только для ДОС</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="tns:string-500">
							<xs:enumeration value="Продавец"/>
							<xs:enumeration value="Покупатель"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:annotation>
		<xs:documentation>Ответы</xs:documentation>
	</xs:annotation>
	<xs:element name="ContractStatusResponse">
		<xs:annotation>
			<xs:documentation>Ответ на Запрос проверки существования документа</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="IsDocExist" type="xs:boolean">
					<xs:annotation>
						<xs:documentation>Флаг</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocListResponse">
		<xs:annotation>
			<xs:documentation>Ответ на Запрос списка документов лесопользователя</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Document" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Документ</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id" type="tns:string-500" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Внутренний id документа в системе</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="BuyerMember" type="tns:ContractPartyType">
								<xs:annotation>
									<xs:documentation>Сторона сделки (Покупатель / Арендатор / Исполнитель)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="SellerMemberType">
								<xs:annotation>
									<xs:documentation>Сторона сделки (Продавец / ОИВ)</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="SellerParty" type="tns:ContractPartyType">
											<xs:annotation>
												<xs:documentation>Продавец</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ExecutiveAgency" type="tns:ExecutiveAgencyType">
											<xs:annotation>
												<xs:documentation>ОИВ</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element name="DocNum" type="tns:string-500">
								<xs:annotation>
									<xs:documentation>Номер документа</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:choice>
								<xs:element name="ContractLease">
									<xs:annotation>
										<xs:documentation>Договор аренды</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="StartDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата заключения /поступления</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SignDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата подписания ОИВ</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="ContractForestWorks">
									<xs:annotation>
										<xs:documentation>Государственный контракт (или государственное задание)</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="StartDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата заключения /поступления</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SignDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата подписания ОИВ</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="InputDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата ввода данных</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="ContractReport">
									<xs:annotation>
										<xs:documentation>Отчет об использовании</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="StartDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата заключения /поступления</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SignDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата подписания ОИВ</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="InputDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата ввода данных</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="ContractHardwoodDeal">
									<xs:annotation>
										<xs:documentation>Декларация о сделке с древесиной</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="BuyerSignDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата подписания Покупателем</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SellerSignDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата подписания Продавцом</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="Volume" type="tns:VolumeType">
												<xs:annotation>
													<xs:documentation>Объем, м3</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="MarkingCount" type="xs:int">
												<xs:annotation>
													<xs:documentation>Количество маркировок</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="DeclaredBy" minOccurs="0" maxOccurs="1">
												<xs:annotation>
													<xs:documentation>Автор декларации (продавец/ покупатель)</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="tns:string-500">
														<xs:enumeration value="Продавец"/>
														<xs:enumeration value="Покупатель"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="ContractPlantsSale">
									<xs:annotation>
										<xs:documentation>Договор купли-продажи</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="StartDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата заключения /поступления</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SignDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата подписания ОИВ</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="ContractForestDeclaration">
									<xs:annotation>
										<xs:documentation>Лесная декларация</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="InputDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата ввода данных</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SignDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата подписания ОИВ</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="StartDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата заключения /поступления</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ReportMonth" type="xs:gMonth">
												<xs:annotation>
													<xs:documentation>Отчетный месяц</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ReportYear" type="xs:gYear">
												<xs:annotation>
													<xs:documentation>Отчетный год</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="ContractRight">
									<xs:annotation>
										<xs:documentation>Право пользования</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="InputDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата ввода данных</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SignDate" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата подписания ОИВ</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:choice>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocVidimusResponse">
		<xs:annotation>
			<xs:documentation>Ответ на Запрос выписки по документу</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="InputDate" type="xs:date">
					<xs:annotation>
						<xs:documentation>Дата ввода в ЕГАИС</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ExecutiveAgency" type="tns:ExecutiveAgencyType">
					<xs:annotation>
						<xs:documentation>ОИВ</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element name="ContractLeaseVidimus">
						<xs:annotation>
							<xs:documentation>Договор аренды</xs:documentation>
