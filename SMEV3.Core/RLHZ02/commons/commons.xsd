								<xs:element name="TermContract" type="xs:boolean" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Срочный договор</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="DocKind" type="tns:DocKind">
									<xs:annotation>
										<xs:documentation>Вид документа</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Volume" type="tns:WoodVolumeType">
									<xs:annotation>
										<xs:documentation>Объем древесины</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Location" type="tns:LocationType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Местоположение</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Executor" type="tns:ContractPartyType">
									<xs:annotation>
										<xs:documentation>Исполнитель</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Area" type="tns:AreaType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Номера участков </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ContractForestDeclarationVidimus">
