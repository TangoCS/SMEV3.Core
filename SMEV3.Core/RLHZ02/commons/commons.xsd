										<xs:documentation>Реквизиты договора</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="ConclusionDate" type="xs:date">
									<xs:annotation>
										<xs:documentation>Дата заключения</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="RegistryNum" type="xs:string">
									<xs:annotation>
										<xs:documentation>Номер регистрации</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Volume" type="tns:WoodVolumeType">
									<xs:annotation>
										<xs:documentation>Объем древесины</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Area" type="tns:AreaType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Номера участков </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Location" type="tns:LocationType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Местоположение</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ContractPlantsSaleVidimus">
						<xs:annotation>
							<xs:documentation>Договор купли-продажи</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Contract" type="tns:ContractType">
									<xs:annotation>
										<xs:documentation>Контракт</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Volume" type="tns:WoodVolumeType">
									<xs:annotation>
										<xs:documentation>Объем древесины</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Location" type="tns:LocationType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Местоположение</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Buyer" type="tns:ContractPartyType">
									<xs:annotation>
										<xs:documentation>Покупатель</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="CoherentGovDoc" type="tns:DocDataType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Связанный ГК</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ContractForestWorksVidimus">
						<xs:annotation>
							<xs:documentation>Государственный контракт (или государственное задание)</xs:documentation>
