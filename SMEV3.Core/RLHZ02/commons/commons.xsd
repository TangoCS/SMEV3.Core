										<xs:documentation>Связанный ГК</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ContractRightVidimus">
						<xs:annotation>
							<xs:documentation>Право пользования</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ViolationReason" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Причина нарушения срока внесения в ЕГАИС</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Contract" type="tns:ContractType">
									<xs:annotation>
										<xs:documentation>Контракт</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="StateRegistrationNum" type="xs:string" minOccurs="0">
